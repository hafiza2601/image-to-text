{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js"
        }
    ],
    "functions": {
        "src/utils/ocrService.js": {
            "includeFiles": "node_modules/tesseract.js-core/*"
        }
    }
}